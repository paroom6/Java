package Exam;

import java.util.ArrayList;
import java.util.List;

public class Example10 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		List<String> list = new ArrayList<String>();
		
		list.add("사과");
		list.add("딸기");
		list.add("바나나");
		System.out.println(list);
		list.add("포도");
		list.add("체리");
		System.out.println(list);
		
		list.add(2,"망고");
		list.add(3,"수박");
		System.out.println(list);
		
	}

}
